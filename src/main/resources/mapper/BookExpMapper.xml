<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.athome.mybatis.mapper.BookExpMapper">
    <insert id="save" parameterType="com.athome.pojo.Book">
        insert into book(`name`,total) values(#{name},#{total})
    </insert>
</mapper>